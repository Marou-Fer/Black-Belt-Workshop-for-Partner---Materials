{
    "eula": "",
    "vendor": "SAP",
    "license": "",
    "id": "com.sap.sac.sample.echarts.gantt",
    "icon": "data:image/png;base64,",
    "version": "1.0.0",
    "supportsMobile": true,
    "supportsLinkedAnalysisFilterOnSelection": true,
    "name": "EChart Gantt Chart",
    "newInstancePrefix": "EChartsGantt",
    "description": "A Sample Gantt Chart for reading data only",
    "webcomponents": [
      {
        "kind": "main",
        "tag": "com-sap-sac-sample-echarts-gantt",
        "url": "/main.js",
        "integrity": "",
        "ignoreIntegrity": true
      }
    ],
    "properties": {
      "width": {
        "type": "integer",
        "default": 600
      },
      "height": {
        "type": "integer",
        "default": 320
      }
    },
    "events": {},
    "methods": {
          "getDataSource": {
              "parameters": [],
              "body": "return this.dataBindings.getDataBinding('dataBinding').getDataSource();",
              "returnType": "DataSource"
          }
    },
    "types": {},
    "dataBindings": {
      "dataBinding": {
        "feeds": [
          {
            "id": "dimensions",
            "description": "Dimensions",
            "type": "dimension"
          },
          {
            "id": "measures",
            "description": "Measures",
            "type": "mainStructureMember"
          }
        ]
      }
    }
  }
  